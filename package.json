{
  "name": "protege-lecture-plus",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Plateforme de Réservation du Centre de Lecture - Protégé QV ONG",
  "author": "Protégé QV ONG",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon server/index.js",
    "build": "npm exec vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:backend": "vitest run server/test",
    "test:frontend": "vitest run src/test",
    "prebuild": "node scripts/install-rollup-native.js",
    "init:admin": "node scripts/init-admin.js",
    "init:db": "node scripts/init-db.js",
    "migrate": "node server/migrations/migrate.js",
    "seed": "node server/migrations/seed.js",
    "import-excel": "node server/scripts/import-excel.js",
    "start": "node server/index.js",
    "sitemap": "node scripts/generate-sitemap.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.2",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dompurify": "^3.2.7",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "framer-motion": "^12.23.24",
    "helmet": "^7.1.0",
    "html2pdf.js": "^0.12.1",
    "i18next": "^25.6.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemon": "^3.0.2",
    "papaparse": "^5.5.3",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.64.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.0.0",
    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.2.1",
    "sql.js": "^1.13.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.36.0",
    "vite": "^7.1.9",
    "vitest": "^1.0.4"
  }
}
